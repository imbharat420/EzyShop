                        </ul>
                    </div>
                </div>
                <!-- End User Event Area -->
            </div>
        </div>
        <!-- ...:::End User Event Section:::... -->

    <%- include('partials/profileMenu') %>
      <%- include('partials/mobileMenu') %>
      <%- include('components/categories') %>

        <!-- ...:::Start Billing Info Section:::... -->
        <div class="billing-info section-gap-top-25">
            <div class="container px-0">
                <div class="billing-information-card">

                    <span class="billing-information-title">Billing Information</span>
                    <ul class="billing-information-lists">
                        <li class="billing-information-list"> <span> Name</span>
                            <span>SUHA JANNAT</span>
                        </li>
                        <li class="billing-information-list"> <span> Email Address</span>
                            <span>care@example.com</span>
                        </li>
                        <li class="billing-information-list"> <span> Phone</span>
                            <span>+880 000 111 222</span>
                        </li>
                        <li class="billing-information-list"> <span> Shipping
								Address</span> <span>28/C Green Road, BD</span> </li>
                    </ul>

                    <span class="shipping-method-title">Shipping Method Choose</span>
                    <ul class="shipping-method-lists">
                        <li class="shipping-method-list">
                            <input id="fastShipping" type="radio" name="selector" checked="">
                            <label for="fastShipping">Fast Shipping<span>1 days delivary for
									$1.0</span></label>
                            <div class="check"></div>
                        </li>
                        <li class="shipping-method-list">
                            <input id="normalShipping" type="radio" name="selector">
                            <label for="normalShipping">Reguler<span>3-7 days delivary for
									$0.4</span></label>
                            <div class="check"></div>
                        </li>
                        <li class="shipping-method-list">
                            <input id="courier" type="radio" name="selector">
                            <label for="courier">Courier<span>5-8 days delivary for $0.3</span></label>
                            <div class="check"></div>
                        </li>
                    </ul>

                    <div class="payment-card">
                        <span class="total-price">$<span class="counter">39.84</span></span>
                        <a class="btn-payment" href="/">Confirm &amp; Pay</a>
                    </div>

                </div>

            </div>
        </div>
        <!-- ...:::End Billing Info Section:::... -->

        <!-- ...:::Start User Event Section:::... -->
        <div class="user-event-section">
            <!-- Start User Event Area -->
            <div class="col pos-relative">

                <div class="user-event-area">
                    <div class="user-event user-event--left">
                        <a area-label="event link icon" href="index" class="event-btn-link"><i
                        class="icon icon-carce-home"></i></a>
                        <a area-label="wishlist icon" href="wishlist" class="event-btn-link"><i
                        class="icon icon-carce-heart"></i></a>
                    </div>
                    <div class="user-event user-event--center">
                        <a area-label="cart icon" href="cart" class="event-btn-link"><i
                        class="icon icon-carce-cart"></i></a>
                    </div>
                    <div class="user-event user-event--right">
                        <a area-label="order icon" href="order" class="event-btn-link"><i
                        class="icon icon-carce-compare"></i></a>
                        <a area-label="chat icon" href="chat" class="event-btn-link"><i
                        class="icon icon-carce-bubbles2"></i></a>
                    </div>
                </div>
            </div>
            <!-- End User Event Area -->
        </div>
        <!-- ...:::End User Event Section:::... -->

        <footer class="footer-section"></footer>
